<!-- Testimonial End -->
@section scripts{

    <script>
        $(".carousel-item").first().addClass("active");
    </script>
    <script src="/js/jquery.ticker.js"></script>
    <script>
        $(function() {
            $('.ticker').ticker();
        });
    </script>
    @if (culture.Name == "en")
    {

        <script>
            $('.customer-logos').slick({
                slidesToShow: 6,
                rtl: false,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 1500,
                arrows: false,
                dots: false,
                pauseOnHover: false,
                responsive: [{
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 4
                    }
                }, {
                    breakpoint: 520,
                    settings: {
                        slidesToShow: 3
                    }
                }]
            });
        </script>
        <script>
            $('.team-members').slick({
                slidesToShow: 3,
                rtl: false,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 1500,
                arrows: false,
                dots: false,
                pauseOnHover: false,
                responsive: [{
                    breakpoint: 868,
                    settings: {
                        slidesToShow: 2
                    }
                }, {
                    breakpoint: 520,
                    settings: {
                        slidesToShow: 1
                    }
                }]
            });
        </script>
    }
    else
    {
        <script>
            $('.customer-logos').slick({
                slidesToShow: 6,
                rtl: true,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 1500,
                arrows: false,
                dots: false,
                pauseOnHover: false,
                responsive: [{
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 4
                    }
                }, {
                    breakpoint: 520,
                    settings: {
                        slidesToShow: 3
                    }
                }]
            });
        </script>
        <script>
            $('.team-members').slick({
                slidesToShow: 3,
                rtl: true,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 1500,
                arrows: false,
                dots: false,
                pauseOnHover: false,
                responsive: [{
                    breakpoint: 868,
                    settings: {
                        slidesToShow: 2
                    }
                }, {
                    breakpoint: 520,
                    settings: {
                        slidesToShow: 1
                    }
                }]
            });
        </script>
    }

}